/* ------------------------------------------------------------ *\
	Helpers
\* ------------------------------------------------------------ */

/*  Clear  */
.shell-alt:after,
.section-card-single.section-card-single-alt .section-head:after,
.update-items:after,
.row:after,
.clear:after { content: ''; line-height: 0; display: table; clear: both; }
.visible-mobile { display: none !important; }

/*  Notext  */

.notext { white-space: nowrap; text-indent: 100%; text-shadow: none; overflow: hidden; }
.notext-old { font-size: 0; line-height: 0; text-indent: -4000px; }


/*  Hidden  */

[hidden],
.hidden { display: none; }


/*  Alignleft  */

.alignleft { float: left; }

/*  Alignright  */

.alignright { float: right; }


/*  Disabled  */

[disabled],
.disabled { cursor: default; }

/* Transition color */
.widget-nav a,
.btn,
.nav .nav-body a { transition: color .3s; }

/* Transition background */
.btn { transition: background .3s; }

/* Transition opacity */
.isotope-items.isotope-items-secondary .item,
.card .card-title a,
.slider-intro .slider-next,
.nav .btn-gotop,
.btn { transition: opacity .3s; }

/* Transition all */
.widget-nav a:before,
.nav-body,
.nav .nav-body a:before { transition: all .3s; }

/* placeholder */
.placeholder { position: relative; overflow: hidden; }
.placeholder img { display:block; width:100%; }
.placeholder-false { opacity: 0; visibility: hidden; z-index: -1; }
.placeholder-label { position: absolute; top:0; left:0; background: rgba(255,255,255, .8); width:193px; line-height:127px; text-align:center; }

