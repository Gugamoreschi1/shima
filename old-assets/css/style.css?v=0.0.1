/* ------------------------------------------------------------ *\
	Button
\* ------------------------------------------------------------ */

.btn { display: inline-block; vertical-align: middle; line-height: 18px; -webkit-appearance: none; -moz-appearance: none; appearance: none; text-decoration: none; padding:18px 68px; }
.btn:hover { opacity: .7; }

.btn.btn-warning { background:#FF9100; font-weight:500; transition: all .3s; color: #3E2723;}
.btn.btn-success { background:#00e676; }
.btn.btn-block { display:block; text-align:center; }

.btn.btn-warning:hover { opacity: 1; background:#d17906; }

.btn-menu { text-decoration: none; text-transform: uppercase; display: inline-block; color: #fff; }
.btn-menu .menu-icon { display: inline-block; text-align:center; vertical-align: middle; }
.btn-menu .text { text-align:center; display: inline-block; vertical-align: middle; }
