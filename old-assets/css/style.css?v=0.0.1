.widget-nav li { padding-bottom:24px; padding-right:52px; }
.widget-nav a { font-size:1.5em; line-height: 1em; color: #90A4AE; text-decoration: none; position: relative; letter-spacing: -0.5px; }

.widget-nav a:before { content: ''; position: absolute; top: 50%; margin-top: -1px; right: -72px; width: 32px; height: 2px; background: #0B202B; opacity: 0; }


.widget-nav a:hover { color: #0B202B; }
.widget-nav a:hover:before { opacity: 1; right: -52px; }

.widget-nav a.active { color: #0B202B; }
.widget-nav a.active:before { opacity: 1; right: -52px; }

.widget-nav .active a { color: #0B202B; }
.widget-nav .active a:before { opacity: 1; right: -52px; }

.widget-nav.widget-nav-alt a:before { background:#5fa3d1; }

.widget-nav.widget-nav-secondary a { color: #72909d; }
.widget-nav.widget-nav-secondary a:hover { color: #fff; }
.widget-nav.widget-nav-secondary a:before { background:#fff; }

.widget-nav.widget-nav-secondary a.active { color: #fff; }

@media (max-width: 1200px) {
	.widgets { padding:10px 0; display:block; }
	.widget-nav { text-align:left; padding: 0 20px; }
	.widget-nav ul { display: table; width:100%; table-layout: fixed; }
	.widget-nav li { padding:0; display: table-cell; vertical-align: top; text-align:center; }
	.widget-nav a { padding:10px 0; display:block; font-size:20px; }
	.widget-nav a:before { display:none; }
}

@media (max-width: 767px) {
	.widget-nav ul { display:block; }
	.widget-nav li { display:block; }
}
