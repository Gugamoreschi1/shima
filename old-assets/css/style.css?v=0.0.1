.card.card-alt { background: url(images/black-tile@2x.png) no-repeat 0 0; background-size: cover; background-position:center center; display: table; width:100%; }
.card.card-alt .card-inner { display: table-cell; vertical-align: middle; text-align:center; text-decoration: none; }
.card.card-alt .card-inner h4 { font-weight: 400; color: #855504; letter-spacing: 2.8px; }

/* ------------------------------------------------------------ *\
	Cards Secondary
